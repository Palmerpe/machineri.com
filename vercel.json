{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "css/**",
        "use": "@vercel/static"
      },
      {
        "src": "img/**",
        "use": "@vercel/static"
      },
      {
        "src": "js/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/contact.js" },
      { "src": "/", "dest": "/index.html" },
      { "src": "/contacto", "dest": "/contacto.html" },
      { "src": "/fabricacion", "dest": "/fabricacion.html" },
      { "src": "/mantenimiento-industrial", "dest": "/mantenimiento-industrial.html" },
      { "src": "/montaje-desmontaje", "dest": "/montaje-desmontaje.html" },
      { "src": "/nosotros", "dest": "/nosotros.html" },
      { "src": "/obras-civiles", "dest": "/obras-civiles.html" },
      { "src": "/puentes", "dest": "/puentes.html" },
      { "src": "/tanques", "dest": "/tanques.html" },
      { "src": "/techos", "dest": "/techos.html" },
      { "src": "/mantenimiento-barcos", "dest": "/mantenimiento-barcos.html" },
      { "src": "/electricidad", "dest": "/electricidad.html" },
      { "src": "/escaleras", "dest": "/escaleras.html" },
      { "src": "/mineria", "dest": "/mineria.html" }
    ]
  }
  
  